removeGrppcMulti(removeAll: boolean = false) {
  console.log('removeGrppcMulti called. removeAll =', removeAll);
  console.log('selectedItemGrppcMulti:', this.selectedItemGrppcMulti);
  console.log('selectedRowGrppcMulti:', this.selectedRowGrppcMulti);

  if (removeAll) {
    // Remove all GRPPCs
    this.ptfActor.grppcMulti = [];
    this.selectedRowGrppcMulti = null;
    this.selectedItemGrppcMulti = null;
    this.selectedGrppcMulti = [];

    if (this.grppcMultiDropDown) {
      this.grppcMultiDropDown.clearModel();
    }
    return;
  }

  // Normal single remove case
  if (this.selectedRowGrppcMulti == null) {
    this.errorPopUp('Please select at least one choice to remove.');
    return;
  }

  this.ptfActor.grppcMulti.splice(this.selectedRowGrppcMulti, 1);
  this.selectedRowGrppcMulti = null;
  this.selectedItemGrppcMulti = null;
}

public List<String> getLevels() {

    StoredProcedure proc =
            new StoredProcedure(mJdbcTemplate,"kstwactorbdr.p_grp_pc_lvl_list") {};

    proc.declareParameter(
            new org.springframework.jdbc.core.SqlOutParameter("p_result", OracleTypes.CURSOR));

    proc.compile();

    Map<String,Object> out = proc.execute();

    // default cursor mapping = List< Map >
    List<Map<String,Object>> rows = (List<Map<String,Object>>) out.get("p_result");

    // since only 1 column â†’ first col only
    return rows.stream()
               .map(m -> m.values().iterator().next().toString())
               .collect(Collectors.toList());
}
