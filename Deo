@Override
public List<String> getLevels() {

    StoredProcedure proc =
            new StoredProcedure(mJdbcTemplate,"kstwactorbdr.p_grp_pc_lvl_list") {};

    proc.declareParameter(
            new org.springframework.jdbc.core.SqlOutParameter("p_result", OracleTypes.CURSOR));

    proc.compile();

    Map<String,Object> out = proc.execute();

    // default cursor mapping = List< Map >
    List<Map<String,Object>> rows = (List<Map<String,Object>>) out.get("p_result");

    // since only 1 column â†’ first col only
    return rows.stream()
               .map(m -> m.values().iterator().next().toString())
               .collect(Collectors.toList());
}
